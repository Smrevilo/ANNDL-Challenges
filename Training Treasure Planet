rix\n    cm = confusion_matrix(ds_gt, ds_predictions)\n    \n    # Create labels combining confusion matrix values\n    labels = np.array([f\"{num}\" for num in cm.flatten()]).reshape(cm.shape)\n    \n    # Plot the confusion matrix with class labels\n    plt.figure(figsize=(8, 6))\n    sns.heatmap(cm, annot=labels, fmt='', xticklabels=labels, yticklabels=labels, cmap='Blues')\n    plt.xlabel('True labels')\n    plt.ylabel('Predicted labels')\n    plt.show()","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-11-23T12:24:18.421167Z","iopub.execute_input":"2024-11-23T12:24:18.421439Z","iopub.status.idle":"2024-11-23T12:24:18.429861Z","shell.execute_reply.started":"2024-11-23T12:24:18.421413Z","shell.execute_reply":"2024-11-23T12:24:18.428572Z"}},"outputs":[],"execution_count":null},{"cell_type":"code","source":"evaluations(model, ds=X_val, y_ds=y_val, labels=LABELS, name='validation')","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-11-23T12:24:20.021107Z","iopub.execute_input":"2024-11-23T12:24:20.021460Z","iopub.status.idle":"2024-11-23T12:25:11.147156Z","shell.execute_reply.started":"2024-11-23T12:24:20.021430Z","shell.execute_reply":"2024-11-23T12:25:11.145985Z"}},"outputs":[],"execution_count":null},{"cell_type":"code","source":"# Get the weights of the last two dense layers and the output layer\ndense_layer_1_weights = model.get_layer('Dense_layer_0').get_weights()\ndense_layer_2_weights = model.get_layer('Dense_layer_1').get_weights()\ndense_layer_3_weights = model.get_layer('Dense_layer_2').get_weights()\noutput_layer_weights = model.get_layer('output_layer').get_weights()\n\n# Save the weights to files\nnp.savez('dense_layer_1_weights.npz', *dense_layer_1_weights)\nnp.savez('dense_layer_2_weights.npz', *dense_layer_2_weights)\nnp.savez('dense_layer_3_weights.npz', *dense_layer_3_weights)\nnp.savez('output_layer_weights.npz', *output_layer_weights)","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-11-23T12:25: